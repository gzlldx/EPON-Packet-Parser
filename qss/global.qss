QFrame#symbolFrame
{
    border-radius: 3px;
    color: white;
    background-color: #505050;
}

QToolBar
{
	border: none;
	border-bottom: 1px solid white;
	background-color: #707070;
}

QToolButton
{
    color: #e0e0e0;
}

QToolButton:hover
{
	margin: 1px;
	margin-bottom: 2px;
    border-radius: 3px;
    background-color: #909090;
}

QToolButton:pressed
{
	padding-left: 2px;
	padding-top:  2px;
    background-color: #4060a0;
}

QCheckBox
{
    color: #e0e0e0;
}

